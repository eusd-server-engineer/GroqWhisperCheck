# Groq API Configuration
# Copy this file to .env and add your actual API key

# Required: Your Groq API key
GROQ_API_KEY=your_groq_api_key_here

# Optional: Default models
GROQ_DEFAULT_MODEL=groq/compound
GROQ_DEFAULT_WHISPER_MODEL=whisper-large-v3-turbo

# Optional: Account tier (free or developer)
# Affects file size limits for Whisper
GROQ_ACCOUNT_TIER=free

# Optional: Default temperature for chat completions
GROQ_DEFAULT_TEMPERATURE=0.7

# Optional: Default max tokens for responses
GROQ_DEFAULT_MAX_TOKENS=2000